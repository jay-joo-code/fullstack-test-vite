  "scripts": {
    "start": "node server/build/app.js",
    "heroku-postbuild": "npm run build:frontend && npm run build:server",
    "build:frontend": "cd frontend && npm run build && cd ..",
    "build:server": "cd server && npm run build && cd ..",
    "postinstall": "npm run install:frontend && npm run install:server",
    "install:frontend": "cd frontend && npm install && cd ..",
    "install:server": "cd server && npm install && cd ..",
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:server\"",
    "dev:frontend": "cd frontend && npm run dev && cd ..",
    "dev:server": "cd server && npm run dev && cd ..",
    "lint": "eslint . --ext .js --ext .jsx --ext .ts --ext .tsx --fix"
