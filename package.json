{
  "name": "carriage-web",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "dependencies": {
    "@types/jquery": "^3.3.33",
    "babel-eslint": "^10.1.0",
    "express": "^4.17.1",
    "google-auth-library": "^6.0.0",
    "react-google-login": "^5.1.3",
    "typescript": "^4.0.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "concurrently": "^6.0.2",
    "eslint": "^7.25.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.23.2"
  },
  "scripts": {
    "start": "node server/build/app.js",
    "heroku-postbuild": "npm run build",
    "build": "npm run build:frontend && npm run build:server",
    "build:frontend": "cd frontend && npm run build && cd ..",
    "build:server": "cd server && npm run tsc && cd ..",
    "postinstall": "npm run install:frontend && npm run install:server",
    "install:frontend": "cd frontend && npm install && cd ..",
    "install:server": "cd server && npm install && cd ..",
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:server\"",
    "dev:frontend": "cd frontend && npm run dev && cd ..",
    "dev:server": "cd server && npm run dev && cd ..",
    "lint": "eslint . --ext .js --ext .jsx --ext .ts --ext .tsx --fix"
  },
